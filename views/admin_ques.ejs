<div class="formmy">
    <label for="name">Name: </label>
    <input type="text" placeholder="ques name" id="name" class="name">

    <label for="ques">Ques: </label>
    <textarea cols="30" rows="10" type="text" placeholder="ques" id="ques" class="ques"></textarea>

    <label for="input">TestCase Input: </label>
    <input type="text" placeholder="TestCase Input" id="input" class="input">

    <label for="output_compare">Name: </label>
    <input type="text" placeholder="output_compare" id="output_compare" class="output_compare">

    <button onclick="submitQues()">submit</button>
</div>

<script>
    async function submitQues(){
        var name = document.getElementById('name').value;
        var ques = document.getElementById('ques').value;
        var input = document.getElementById('input').value;
        var output_compare = document.getElementById('output_compare').value;
        var data = {
            name: name,
            ques: ques,
            input: input,
            output_compare: output_compare
        };
        await fetch('/admin/question/submit', {
            method: 'POST',
            body: JSON.stringify(data),
            headers: {
                'Content-Type': 'application/json'
            }
        }).then(async function(response){
            console.log(await response.json());
        });
    }
</script>

<style>
    .formmy {
        display: flex;
        flex-direction: column;
        width: 50vw;
    }
</style>